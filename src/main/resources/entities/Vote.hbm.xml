<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.icerebro.hackFinale.entities.Vote" table="votes" schema="public" catalog="hackdb">

        <id name="id" column="id">
            <generator class="increment"/>
        </id>

        <property name="votename" column="votename"/>
        <property name="votecategory" column="votecategory"/>
        <property name="closed" column="closed"/>
    </class>
</hibernate-mapping>